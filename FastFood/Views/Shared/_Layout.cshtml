<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="~/js/jquery-1.8.3.min.js"><\/script>')</script>

    <link rel="stylesheet" href="~/bootstrap 4/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="~/Content/website14.css" />
    <link rel="stylesheet" href="~/Content/footer.css" />
    <link rel="stylesheet" href="~/Content/parallaxitems3.css" />
    <link rel="stylesheet" href="~/Content/meta-data4.css" />



    <title>@ViewBag.Title</title>
</head>
<body>


    @Html.Partial("ParallaxLogo")

    <div id="upper-body">





        <nav class="navbar navbar-expand-lg navbar-dark bg-dark sticky-top">
            <a class="navbar-brand" asp-controller="Home" asp-action="Index">FastFood</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">

                    @await Component.InvokeAsync("ShoppingCartSummary")

                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link">About</a>
                    </li>
                    <li class="nav-item">


                        @await Component.InvokeAsync("CategoryMenu")
                </ul>
                @await Html.PartialAsync("LoginPartial")
                <form class="form-inline my-2 my-lg-0" asp-controller="FastFood" asp-action="Search" method="get">
                    <input class="form-control mr-sm-2" type="text" placeholder="Search" aria-label="Search" name="searchString" value="@ViewData["searchString"]">
                    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
                </form>
            </div>
        </nav>





        <div>
            @RenderBody()
        </div>


        @Html.Partial("ParalaxItems")

    </div>



    




    @Html.Partial("Footer")






















    <script src="~/js/parallaxlogo4.js"></script>
    <script src="~/js/ParallaxBlog.js"></script>
    <script src="~/js/ParallaxCategory1.js"></script>

    @RenderSection("masterjs", required: false)

    @*<script src="~/js/jquery.min.js"></script>
        <script src="~/js/jquery-1.8.3.min.js"></script>*@
    <script src="~/bootstrap 4/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="~/js/jquery-3.1.0.js"></script>
    <script src="~/bootstrap 4/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="~/bootstrap 4/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>




</body>
</html>
